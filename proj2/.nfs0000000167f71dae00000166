done[m[30;5H[K[31;1H[93mecho[m[31;5H[K[32;1H[K[33;1H[93mfor[m server [93min[m [93m"[m[38;5;81m${servers[[m@[38;5;81m]}[m[93m"[m; [93mdo[m[33;34H[K[34;1H   [34;4H[K[34;4H[?25h[?25l [96m# strip protocol and trailing slashes[m
    [1m[96mclean_server[m=[38;5;81m$([m[93mecho[m[95m [m[93m"[m[35;26H[K[35;26H[?25h[?25l[38;5;81m$server[m[93m"[m[95m [m[93m|[m[38;5;224m [m[93msed[m[38;5;224m -E [m[93m'[m[95ms~https?://~~[m[93m'[m[38;5;224m [m[93m|[m[38;5;224m [m[93msed[m[38;5;224m [m[93m'[m[95ms:/$::[m[93m'[m[38;5;81m)[m
    [93mecho[m[95m -n [m[93m"[m[95m$[m[36;15H[K[36;15H[?25h[?25l[38;5;81m$clean_server[m[95m,[m[93m"[m[37;1H[K[38;1H    [96m# First ping to get IP[m[38;27H[K[39;1H    [1m[96mfirst_output[m=[38;5;81m$([m[38;5;224m.[m[39;21H[K[39;21H[?25h[?25l[38;5;224m/webclient [m[93m"[m[38;5;81m$clean_server[m[93m"[m[38;5;224m -ping [m[95m2[m[93m>[m[38;5;224m/dev/null[m[38;5;81m)[m
    [1m[96mip[m=[38;5;81m$([m[93mecho[m[95m [m[93m"[m[38;5;81m$f[m[40;18H[K[40;18H[?25h[?25l[38;5;81mirst_output[m[93m"[m[95m [m[93m|[m[38;5;224m awk [m[93m'[m[95m{print $1}[m[93m'[m[38;5;81m)[40;12H[?25h[?25l[1;40r[m[1;1H[3M[1;41r[38;5H[93mecho[m[95m -n [m[93m"[m[38;5;81m$ip[m[93m"[m[40;5H[96m# Store RTTs[m[41;11H[1m(paste) --[m[41;59H[K[40;46H[?25h[?25l[1;40r[1;1H[2M[1;41r[39;5H[1m[96mrtts[m=[38;5;81m()[m
    rtts[93m+=[m[38;5;81m($([m[93mecho[m[95m [m[93m"[m[38;5;81m$first_output[m[93m"[m[95m [m[93m|[m[38;5;224m awk [m[93m'[m[95m{print $3}[m[93m'[m[38;5;81m))[40;20H[?25h[?25l[1;40r[m[1;1H[3M[1;41r[39;5H[93mfor[m i [93min[m [38;5;81m$([m[38;5;224mseq [m[95m2[m[38;5;224m [m[38;5;81m$count)[m[93m;[m [93mdo[m[40;9H[1m[96moutput[m=[38;5;81m$([m[38;5;224m./webclient [m[93m"[m[38;5;81m$clean_server[m[93m"[m[38;5;224m -ping [m[95m2[m[93m>[m[38;5;224m/dev/null[m[38;5;81m)[40;19H[?25h[?25l[1;40r[m[40;1H
[1;41r[40;9H[1m[96mrtt[m=[38;5;81m$([m[93mecho[m[95m [m[93m"[m[38;5;81m$output[m[93m"[m[95m [m[93m|[m[38;5;224m awk [m[93m'[m[95m{print $3}[m[93m'[m[38;5;81m)[?25h[?25l[1;40r[m[1;1H[3M[1;41r[38;9Hrtts[93m+=[m[38;5;81m([m[93m"[m[38;5;81m$rtt[m[93m"[m[38;5;81m)[m
    [93mdone[40;7H[?25h[?25l[m[41;1H[K[1;40r[1;1H[6M[1;41r[35;5H[93mfor[m rtt [93min[m [93m"[m[38;5;81m${rtts[[m@[38;5;81m]}[m[93m";[m [93mdo[36;9Hecho[m[95m -n [m[93m"[m[95m,[m[38;5;81m$rtt[m[93m"
[m    [93mdone
